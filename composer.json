{
  "name": "phpas2/phpas2",
  "type": "library",
  "description": "Implementation of AS2Secure for PHP 7.x and composer-based projects.  Based on code from as2secure.com.",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "keywords": [
    "as2",
    "as2secure",
    "as2securecode",
    "phpas2"
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "https://pear.horde.org"
    },
    {
      "type": "pear",
      "url": "https://pear.php.net"
    }
  ],
  "require": {
    "php": "~5.6.0|~7.0.17",
    "pear-pear.horde.org/Horde_Compress": "~2.1.5",
    "pear-pear.horde.org/Horde_Compress_Fast": "~1.1.1",
    "pear-pear.horde.org/Horde_Mime": "~2.10.2",
    "pear-pear.horde.org/Horde_Util": "~2.5.8",
    "pear-pear.php.net/Mail_mimeDecode": "~1.5.6",
    "zendframework/zend-mail": "^2.8",
    "zendframework/zend-mime": "^2.6"
  },
  "autoload": {
    "psr-4": { "PHPAS2\\": "src/" }
  },
  "bin": [ "bin/AS2Secure.jar" ]
}
