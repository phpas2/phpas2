  "require": {
    "php": "^7.0",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-sodium": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
